# Backend API Configuration
VITE_BACKEND_URL=http://localhost:5000
VITE_API_TIMEOUT=10000

# Application Configuration
VITE_APP_NAME=DocumentOptimizer
VITE_APP_VERSION=1.0.0
VITE_APP_DESCRIPTION=Professional Document Management System

# Feature Flags
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_NOTIFICATIONS=true
VITE_ENABLE_DARK_MODE=true

# External Services
VITE_GOOGLE_ANALYTICS_ID=your-ga-id
VITE_SENTRY_DSN=your-sentry-dsn

# Development Configuration
VITE_DEBUG_MODE=true
VITE_LOG_LEVEL=debug

# File Upload Configuration
VITE_MAX_FILE_SIZE=10485760
VITE_ALLOWED_FILE_TYPES=pdf,doc,docx,txt,jpg,png

# UI Configuration
VITE_THEME_PRIMARY_COLOR=#3B82F6
VITE_THEME_SECONDARY_COLOR=#1E40AF